@GraphQLQuery
method readsText() {
	return "Hello";
}


@GraphQLMutation
method writesText(Text text) {
	return text;
}

@GraphQLQuery
method readsInteger() {
	return 123;
}

@GraphQLMutation
method writesInteger(Integer value) {
	return value;
}

@GraphQLQuery
method readsDecimal() {
	return 123.45;
}

@GraphQLMutation
method writesDecimal(Decimal value) {
	return value;
}

enumerated NativeEnum(Text) {
	NATIVE = "Native";
}

@GraphQLQuery
method readsNativeEnum() {
	return NATIVE;
}

@GraphQLQuery
method readsBlob() {
	return fetchBinaryResource("xyz");
}

@GraphQLQuery
method readsImage() {
	return (Image)(Any)fetchBinaryResource("xyz");
}

