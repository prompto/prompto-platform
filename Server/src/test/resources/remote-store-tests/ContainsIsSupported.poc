storable attribute value: Text;
storable category Stuff(value);

widget ContainsIsSupportedPage extends ReactWidget {
	
	method componentDidMount() {
		s1 = Stuff(value="John");
		s2 = Stuff(value="Johnny");
		s3 = Stuff(value="Lucy");
		store(s1, s2, s3);
		forceRender();
	}
	
	Html method render() {
		cursor = fetch all ( Stuff ) where ( value contains "hn" ) order by (value);
		values = stuff.value for each(stuff in cursor);
		return <div id="root">{values}</div>;
	}
}
