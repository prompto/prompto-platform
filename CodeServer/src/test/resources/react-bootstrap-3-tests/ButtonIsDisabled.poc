native method log(Text value) {
	JavaScript: document.getElementById("out").appendChild(document.createTextNode(value));
}

widget ButtonIsDisabledPage extends ReactWidget {

	method getInitialState() {
		return { disabled: true };
	}
	
    method buttonClicked(ClickEvent event) {
        log("ok!");
    }
    
	Html method render () {
		state = getState();
		return <div>
			<div id="out"></div>
			<Button id="button" disabled={state.disabled} onClick={buttonClicked} >Hello "Button"!</Button>
			</div>;
	}
	
}